{"ast":null,"code":"import Son1 from './components/Son1Com.vue';\nimport Son2 from './components/Son2Com.vue';\nimport { mapState } from 'vuex';\n// console.log(mapState(['count', 'title']))\n\nexport default {\n  name: 'app',\n  created() {\n    // console.log(this.$router) // 没配\n    console.log(this.$store.state.count);\n  },\n  computed: {\n    ...mapState(['count', 'title'])\n  },\n  data: function () {\n    return {};\n  },\n  methods: {\n    handleInput(e) {\n      // 1. 实时获取输入框的值\n      const num = +e.target.value;\n      // 2. 提交mutation，调用mutation函数\n      this.$store.commit('changeCount', num);\n    }\n  },\n  components: {\n    Son1,\n    Son2\n  }\n};","map":{"version":3,"names":["Son1","Son2","mapState","name","created","console","log","$store","state","count","computed","data","methods","handleInput","e","num","target","value","commit","components"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>\n      根组件\n      - {{ title }}\n      - {{ count }}\n    </h1>\n    <input :value=\"count\" @input=\"handleInput\" type=\"text\">\n    <Son1></Son1>\n    <hr>\n    <Son2></Son2>\n  </div>\n</template>\n\n<script>\nimport Son1 from './components/Son1Com.vue'\nimport Son2 from './components/Son2Com.vue'\nimport { mapState } from 'vuex'\n// console.log(mapState(['count', 'title']))\n\nexport default {\n  name: 'app',\n  created () {\n    // console.log(this.$router) // 没配\n    console.log(this.$store.state.count)\n  },\n  computed: {\n    ...mapState(['count', 'title'])\n  },\n  data: function () {\n    return {\n\n    }\n  },\n  methods: {\n    handleInput (e) {\n      // 1. 实时获取输入框的值\n      const num = +e.target.value\n      // 2. 提交mutation，调用mutation函数\n      this.$store.commit('changeCount', num)\n    }\n  },\n  components: {\n    Son1,\n    Son2\n  }\n}\n</script>\n\n<style>\n#app {\n  width: 600px;\n  margin: 20px auto;\n  border: 3px solid #ccc;\n  border-radius: 3px;\n  padding: 10px;\n}\n</style>\n"],"mappings":"A